{
  "name": "list-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:gh-pages": "BASE_URL=/List-Vue/ vue-cli-service build --dest dist_gh_pages",
    "serve:prod": " node web.js",
    "serve:gh-pages": "BASE_URL=/List-Vue/ node web.js",
    "audit:packages": "yarn audit || test $? -eq 0",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e:dev": "testcafe -c 2 chrome tests/e2e",
    "test:e2e:prod": "PORT=5000 testcafe -c 2 chrome,firefox tests/e2e --app \"yarn run serve:prod\"",
    "test:e2e:gh-pages": "PORT=5000 BASE_URL=/List-Vue/ testcafe -c 2 chrome,firefox tests/e2e --app \"yarn run serve:gh-pages\"",
    "test:gh-pages": "yarn run build:gh-pages && yarn run test:e2e:gh-pages",
    "test": "yarn run audit:packages && yarn run lint && yarn run test:unit && yarn run build && yarn run test:e2e:prod"
  },
  "dependencies": {
    "core-js": "^3.4.4",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-plugin-vuex": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-vue": "^5.2.2",
    "sass": "^1.21.0",
    "sass-loader": "^8.0.0",
    "testcafe": "^1.8.0",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "resolutions": {
    "handlebars": ">=4.5.3",
    "lodash": ">=4.17.12",
    "mixin-deep": "^1.3.2",
    "set-value": "^2.0.1",
    "serialize-javascript": ">=2.1.1",
    "**/micromatch/braces": "^2.3.2",
    "tree-kill": ">=1.2.2"
  }
}
